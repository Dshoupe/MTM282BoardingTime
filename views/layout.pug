doctype html
html
    head
        title=title
        link(href='style.css' rel='stylesheet')
    body
        ul(class='navbar')
            each item in config.menu
                a(href=item[1])
                    div(class='menu_item')
                        li(id=item[0]) #{item[0]}
        block content
    footer
        hr
        p Copyright &#169;#{new Date().getFullYear()} - Dylan Shoupe, Bob Cortez, Jennifer Monsalve